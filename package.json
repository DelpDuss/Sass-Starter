{
  "name": "sass-starter",
  "version": "1.0.0",
  "description": "Simple SASS Starter",
  "main": "index.js",
  "scripts": {
    "scss": "node-sass --include-path scss src/scss/main.scss dist/css/main.css",
    "watch": "nodemon -e scss -x \"npm run scss\"",
    "sync": "browser-sync start --server --files \"**/*\" ",
    "dev": "concurrently \"npm run watch\" \"npm run sync\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DelpDuss/Sass-starter.git"
  },
  "keywords": [
    "Sass",
    "Starter"
  ],
  "author": "Dlphn Dsst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DelpDuss/Sass-starter/issues"
  },
  "homepage": "https://github.com/DelpDuss/Sass-starter#readme",
  "dependencies": {
    "browser-sync": "^2.26.7",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.3",
    "concurrently": "^5.1.0"
  }
}
